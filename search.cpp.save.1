#include "linear_search_header.h"

/******************************************************************************
* FUNCTION EndOfProgramBanner
*******************************************************************************/

int linearSearch( const vector<string>& list,
					   const string& key )

{
/***************************************************************************
* CONSTANTS
****************************************************************************/


/***************************************************************************
* VARIABLES
****************************************************************************/
int x = 0;

	
	for(size_t index = 0; index < size_t(list.size()); index++)
	{
		if( ((list[index]).find(key)) != string::npos)
		{
			x = index;
		}
		else
		{
			x = -1;
		}
	}
return x;

}




